<h1>
    Create List
</h1>
<hr />
<form method="post" asp-action="createList" asp-controller="home">

    @for (int i = 0; i < 3; i++)
    {
        <input name="model[@i].Name" class="form-control" placeholder="Name" />
        <input name="model[@i].Age" class="form-control" placeholder="Age" />

        <br />
    }

    <button type="submit" class="btn btn-primary">Save</button>
    <a asp-action="Index" class="btn btn-link">Back to list</a>
</form>


